<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ThirdActivity"
    android:gravity="center"
    android:orientation="vertical">

    <TextView
        android:id="@+id/jump_host_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:text="点我跳转到宿主的MainActivity"
        android:layout_marginBottom="20dp"
        android:background="@drawable/buttom_back"/>

    <TextView
        android:id="@+id/plugin_third_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="点我跳转插件Activity2"
        android:background="@drawable/buttom_back"
        android:layout_marginBottom="20dp"/>

    <TextView
        android:id="@+id/plugin_to_second_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="点我跳转插件2"
        android:background="@drawable/buttom_back"/>
</LinearLayout>